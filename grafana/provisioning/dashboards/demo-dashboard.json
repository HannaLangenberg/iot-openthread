      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "P5697886F9CA74929"
          },
          "query": "import \"math\"\r\n\r\nedges = from(bucket: \"measurements\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"sensor\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"rssi_avg\")\r\n  |> group(columns: [\"identifier\", \"neighbor_mac\", \"neighbor_location\"])\r\n  |> mean(column: \"_value\") // Durchschnitt der durchschnittlichen RSSI-Werte\r\n  |> map(fn: (r) => ({\r\n      id: r.identifier + \"-\" + r.neighbor_location,\r\n      source: r.identifier, // Node identifier\r\n      source_name: r.location, // Klarname des Nodes (z.B., roboterlabor)\r\n      target: r.neighbor_location, // Nachbar-MAC-Adresse\r\n      target_name: r.neighbor_location, // Klarname des Nachbarn (z.B., erste_etage_flur)\r\n      mainstat: string(v: math.round(x: r._value * 100.0) / 100.0), // Rundung auf 2 Dezimalstellen\r\n      secondarystat: \"RSSI\",\r\n      thickness: math.round(x: (if r._value < 0.0 then float(v: -r._value) / 10.0 else float(v: r._value) / 10.0) * 100.0) / 100.0, // Gewichtung basierend auf RSSI\r\n      highlighted: r._value < -70.0, // Beispielbedingung für Hervorhebung\r\n      color: if r._value < -80.0 then \"red\" else if r._value < -60.0 then \"orange\" else \"cyan\"\r\n  }))\r\n\r\nnodes = from(bucket: \"measurements\")\r\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\r\n  |> filter(fn: (r) => r[\"_measurement\"] == \"sensor\")\r\n  |> filter(fn: (r) => r[\"_field\"] == \"rssi_avg\")\r\n  |> group(columns: [\"identifier\"])\r\n  |> last() // Holt den neuesten Wert für jeden Identifier\r\n  |> map(fn: (r) => ({\r\n      id: r.identifier,\r\n      title: r.identifier, // Node-Identifier\r\n      name: r.location, // Klarname des Nodes (z.B., roboterlabor)\r\n      mainstat: string(v: r._value),\r\n      secondarystat: \"N/A\", // Oder berechnen, falls relevant\r\n      color: if r._value < -80 then \"red\" else if r._value < -60 then \"orange\" else \"green\",\r\n      icon: \"eye\", // Beispiel, Icon basierend auf deinem Bedarf wählen\r\n      highlighted: r._value < -70 // Beispielbedingung für Hervorhebung\r\n  }))\r\n\r\n// Ausgabe der Nodes und Edges für die Node-Graph-Visualisierung\r\nedges\r\n",
          "refId": "A"
        }
      ],
      "title": "Topologie",
      "type": "nodeGraph"
    }
